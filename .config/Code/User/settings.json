{
  "keyboard.dispatch": "keyCode",
  "workbench.colorTheme": "Default Light+",
  "editor.fontFamily": "'JetBrainsMono Nerd Font Mono', 'monospace', monospace",
  "editor.inlineSuggest.enabled": true,
  "editor.codeLensFontFamily": "'JetBrainsMono Nerd Font Mono', 'monospace', monospace",
  "editor.inlayHints.fontFamily": "'JetBrainsMono Nerd Font Mono', 'monospace', monospace",
  "scm.inputFontFamily": "'JetBrainsMono Nerd Font Mono', 'monospace', monospace",
  "debug.console.fontFamily": "'JetBrainsMono Nerd Font Mono', 'monospace', monospace",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "bash",
      "icon": "terminal-bash"
    },
    "zsh": null
  },
  "jupyter.widgetScriptSources": ["jsdelivr.com", "unpkg.com"],
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "gitlens.codeLens.symbolScopes": ["!Module"],
    "editor.wordBasedSuggestions": false
  },
  "files.associations": {
    "*.ipynb": "python"
  },
  "vscode-neovim.neovimInitVimPaths.linux": "/home/sk/dotfiles/.vimrc",
  "editor.fontSize": 13,
  "python.formatting.provider": "black",
  "jupyter.askForKernelRestart": false,
  "editor.occurrencesHighlight": false,
  "vscode-neovim.neovimClean": true,
  "notebook.output.textLineLimit": 300,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "python": true
  }
}
