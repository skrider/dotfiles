{
  // Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and
  // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
  // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
  // same ids are connected.
  // Example:
  // "Print to console": {
  // 	"prefix": "log",
  // 	"body": [
  // 		"console.log('$1');",
  // 		"$2"
  // 	],
  // 	"description": "Log output to console"
  // }
  "Derivative with Respect to": {
    "prefix": "ddw",
    "body": "\\frac{d}{d $1} $0"
  },
  "Derivative with Respect to Time": {
    "prefix": "ddt",
    "body": "\\frac{d}{dt} $0"
  },
  "Second Derivative with Respect to Time": {
    "prefix": "ddt2",
    "body": "\\frac{d^2}{dt^2} $0"
  },
  "Tilde Symbol": {
    "prefix": "tl",
    "body": "\\tilde{$1}$0"
  },
  "Bold Symbol": {
    "prefix": "bsy",
    "body": "\\boldsymbol $0"
  },
  "Square Root": {
    "prefix": "sqr",
    "body": "\\sqrt{$1}$0"
  },
  "Times": {
    "prefix": "`x",
    "body": "\\times"
  },
  "rationals": {
    "prefix": "rQ",
    "body": "\\Bbb{Q}"
  },
  "fraction": {
    "prefix": "frac",
    "body": "\\frac{$1}{$2} $0"
  },
  "Expected Value": {
    "prefix": "\\E",
    "body": ["\\Bbb{E}[$1]$0"]
  },
  "Lagrangian": {
    "prefix": "Lg",
    "body": ["\\mathscr{L}"]
  },
  "Bracket Set": {
    "prefix": "lrb",
    "body": ["\\{ $0 \\\\}"]
  },
  "Probability Distribution": {
    "prefix": "\\P",
    "body": ["\\Bbb{P}[$1]$0"]
  },
  "Codomain": {
    "prefix": "\\cdm",
    "body": ["\\mathscr{$1}$0"]
  },
  "Covariance": {
    "prefix": "\\Cov",
    "body": ["\\text{Cov}($1, $2)$0"]
  },
  "Correlation": {
    "prefix": "\\Corr",
    "body": ["\\text{Corr}($1, $2)$0"]
  },
  "Standard Deviation": {
    "prefix": "\\std",
    "body": ["\\sigma($1)$0"]
  },
  "Specific Probability Distribution": {
    "prefix": "\\Psub",
    "body": ["\\Bbb{P}_{$1}[$2]$0"]
  },
  "Summation Notation Lower": {
    "prefix": "\\suml",
    "body": ["\\sum_{$1} $0"]
  },
  "Summation Notation Lower Upper": {
    "prefix": "\\sumul",
    "body": ["\\sum_{$1}^{$2} $0"]
  },
  // "Variance": {
  //   "prefix": "\\Var",
  //   "body": ["\\text{Var}($1)$0"]
  // },
  "Permutations": {
    "prefix": "\\pick",
    "body": ["^{$1}P_{$2} $0"]
  },
  "BMatrix": {
    "prefix": "bmat",
    "body": ["\\begin{bmatrix}\n$0\n\\end{bmatrix}"]
  },
  "Partial Derivative": {
    "prefix": "ppd",
    "body": "\\frac{\\partial}{\\partial $1}$0"
  },
  "Cases": {
    "prefix": "cases",
    "body": ["\\begin{cases}\n$0\n\\end{cases}"]
  },
  "KaTeXt": {
    "prefix": "ktx",
    "body": ["\\text{$1}$0"]
  },
  "Inner Product": {
    "prefix": "inp",
    "body": ["\\langle $1, $2 \\rangle $0"]
  },
  "Math Cal Font": {
    "prefix": "mcl",
    "body": ["\\mathcal{$1}$0"]
  },
  "Inverse": {
    "prefix": "v",
    "body": ["$1^{-1} $0"]
  },
  "Optimal": {
    "prefix": "o",
    "body": "$1^\\ast $0"
  },
  "Pseudoinverse": {
    "prefix": "pv",
    // "body": "${TM_CURRENT_LINE/(.)(\\spv)/$2/g}^\\dag"
    "body": "$1^\\dagger $0"
  },
  "Perpendicular": {
    "prefix": "pr",
    "body": "$1^\\bot $0"
  },
  "Transpose": {
    "prefix": "t",
    "body": "$1^T $0"
  },
  "Left Right Brackets": {
    "prefix": "lrb",
    "body": "\\{$0\\\\}"
  },
  "Align": {
    "prefix": "ali",
    "body": ["\\begin{align}", "$0", "\\end{align}"]
  },
  "Align Equals": {
    "prefix": "ae",
    "body": "&="
  },
  "Multiline Align Environment": {
    "prefix": "ema",
    "body": ["$", "\\begin{align*}$0", "\\end{align*}", "$"]
  },
  "Inline Equation": {
    "prefix": "e",
    "body": ["$$1$ $0"]
  },
  "Big Equation": {
    "prefix": "eb",
    "body": ["$ $0 $"]
  },
  "Multiline Equation": {
    "prefix": "em",
    "body": ["$\n$0\n$"]
  },
  // formatting
  "Bold Text": {
    "prefix": "\\tb",
    "body": "\\textbf{$1}$0"
  }
}
